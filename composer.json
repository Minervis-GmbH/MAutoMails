{
  "name": "srag/srautomails",
  "description": "",
  "version": "1.5.5",
  "extra": {
    "ilias_plugin": {
      "id": "srauma",
      "name": "SrAutoMails",
      "ilias_min_version": "5.4.0",
      "ilias_max_version": "6.999",
      "slot": "Services/Cron/CronHook"
    },
    "generate_plugin_readme_template": "SRAG_ILIAS_PLUGIN"
  },
  "type": "project",
  "keywords": [
    "cron",
    "ilias",
    "job",
    "plugin"
  ],
  "homepage": "https://github.com/studer-raimann/SrAutoMails",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "studer + raimann ag - Team Custom 1",
      "email": "support-custom1@studer-raimann.ch",
      "homepage": "https://studer-raimann.ch",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://plugins.studer-raimann.ch/goto.php?target=uihk_srsu_PLSRAUMA"
  },
  "require": {
    "php": ">=7.0",
    "srag/activerecordconfig": ">=0.1.0",
    "srag/custominputguis": ">=0.1.0",
    "srag/devtools": ">=0.1.0",
    "srag/dic": ">=0.1.0",
    "srag/generateplugininfoshelper": ">=0.1.0",
    "srag/librariesnamespacechanger": ">=0.1.0",
    "srag/notifications4plugin": ">=0.1.0",
    "srag/removeplugindataconfirm": ">=0.1.0"
  },
  "autoload": {
    "psr-4": {
      "srag\\Plugins\\SrAutoMails\\": "src/"
    },
    "classmap": [
      "classes/",
      "src/Rule/class.RulesMailConfigGUI.php",
      "src/Rule/class.RuleMailConfigGUI.php"
    ],
    "files": [
      "vendor/srag/dic/src/PHPVersionChecker.php",
      "vendor/srag/dic/src/Ilias7PreWarn.php"
    ]
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "pre-autoload-dump": [
      "srag\\LibrariesNamespaceChanger\\LibrariesNamespaceChanger::rewriteLibrariesNamespaces",
      "srag\\LibrariesNamespaceChanger\\PHP72Backport::PHP72Backport",
      "srag\\GeneratePluginInfosHelper\\SrAutoMails\\GeneratePluginPhpAndXml::generatePluginPhpAndXml",
      "srag\\GeneratePluginInfosHelper\\SrAutoMails\\GeneratePluginReadme::generatePluginReadme"
    ]
  }
}
